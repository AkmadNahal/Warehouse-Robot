import zerorpc

class HelloRPC(object):
	def insert(self, command, x, y, ID):
		print("%s, %s, %s, %s" % (command, x, y, ID))
		return "Received command: %s, %s, %s, %s" % (command, x, y, ID)
	def remove(self, command, x, y, ID):
		print("%s, %s, %s, %s" % (command, x, y, ID))
		return "Received command: %s, %s, %s, %s" % (command, x, y, ID)
s = zerorpc.Server(HelloRPC())
s.bind("tcp://0.0.0.0:4242")
s.run()
